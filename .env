# .env
# OpenAI / Cloud API Keys
OPENAI_API_KEY=

# DeepSeek / Anthropic / other providers (optional)
DEEPSEEK_API_KEY=
ANTHROPIC_API_KEY=

# Optional local provider base URLs (if you want to preconfigure)
OLLAMA_URL=http://localhost:11434
LOCALAI_URL=http://localhost:8080
LMSTUDIO_URL=http://localhost:3000

# Default provider choice: openai | local | ollama | lmstudio | deepseek
ZEPHYRA_DEFAULT_PROVIDER=openai

# Cost estimator settings (USD per 1k tokens)
COST_PER_1K_TOKENS_OPENAI=0.06
COST_PER_1K_TOKENS_DEEPSEEK=0.03

# 1. CRITICAL FIX: Add VITE_ prefix to expose the URL to the frontend.
VITE_SUPABASE_URL=

# 2. RECOMMENDED: Expose the Anon Key with VITE_ prefix. 
# UNCOMMENT AND USE THE KEY BELOW (or the one in your code's comment)
VITE_SUPABASE_ANON_KEY=
